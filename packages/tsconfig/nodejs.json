{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node.js Setup",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "resolveJsonModule": true
  }
}
